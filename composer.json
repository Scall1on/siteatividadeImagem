{
    "name": "siteimagem/image-gallery",
    "description": "Sistema de galeria de imagens com testes",
    "type": "project",
    "require": {
        "php": ">=7.4"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "behat/behat": "^3.13",
        "friends-of-behat/mink-extension": "^2.7",
        "friends-of-behat/mink-browserkit-driver": "^1.6",
        "behat/mink": "^1.13",
        "behat/mink-browserkit-driver": "*",
        "symfony/http-client": "^7.3",
        "symfony/browser-kit": "^7.3"
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    }
}
